@model IEnumerable<BookShop.Data.Publishing>

<table id="publishingsTable" class="table">
    <thead>
        <tr>
            <th class="text-center">Id</th>
            <th class="text-center">Name</th>
            <th class="text-center">Nazwa</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var author in Model)
        {
            <tr>
                <td class="text-center">
                    @author.Id
                </td>
                <td class="text-center">
                    @author.Name
                </td>
                <td class="text-center">
                    <span class="publishingNameForDisplay">@author.NameForDisplay</span>
                </td>
            </tr>
        }
    </tbody>
</table>

<script src="https://cdn.datatables.net/1.10.13/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/myScripts/dataTablesDefault.js"></script>
<script src="~/Scripts/myScripts/adminPublishingIndexPartial.js"></script>

